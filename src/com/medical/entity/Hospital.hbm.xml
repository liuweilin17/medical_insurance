<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
PUBLIC "-//Hibernate/Hibernate Mapping DTD 5.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-5.0.dtd"> 

<hibernate-mapping>  
   <class name="com.medical.entity.Hospital"  table="hospital">  
      <id name="h_id" type="int" column="h_id">
      	
      </id>  
      <!-- 设置主键不能重复和不能为空的属性. -->  
      <property name="h_name" column="h_name"/>  
      <property name="h_password" column="h_password"/>  
      <property name="h_type" column="h_type"/> 
   </class>  
</hibernate-mapping>  


