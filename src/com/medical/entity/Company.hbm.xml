<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
PUBLIC "-//Hibernate/Hibernate Mapping DTD 5.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-5.0.dtd"> 

<hibernate-mapping>  
   <class name="com.medical.entity.Company"  table="company">  
      <id name="c_id" type="int" column="c_id">
      	
      </id>  
      <!-- 设置主键不能重复和不能为空的属性. -->  
      <property name="c_name" column="c_name"/>  
      <property name="c_password" column="c_password"/>
   </class>  
</hibernate-mapping>  


